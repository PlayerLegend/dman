{
    // Use IntelliSense to learn about possible attributes.
    // ...existing code...
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch dman --output -",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/util/dman",
            "args": [
                "--output",
                "-"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "(gdb) dman toggle",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/util/dman",
            "args": [
                "--input",
                "/tmp/test",
                "--toggle",
                "Laptop"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "(gdb) dman list config outputs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/util/dman",
            "args": [
                "--list-config-outputs",
                "/tmp/test",
                "--list-active-outputs"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}